        	<!--属性信息索引-->
        	<select.property.indexes>1,2</select.property.indexes>	
			<!--分类偏好索引-->
			<class.pref.index>3</class.pref.index>
        	<!--输出排序方法（随机排序）-->
        	<Appconf.filler.sortway>2</Appconf.filler.sortway>
        	<!--reduce个数-->
        	<hadoop.reduce.num>50</hadoop.reduce.num>
       		<!--map大小-->
        	<hadoop.map.maxsplitsizejob>64</hadoop.map.maxsplitsizejob>
			<!--补白下限（随机排序）-->
        	<Appconf.filler.minnum>20</Appconf.filler.minnum>
			<!--补白上限--> 	
        	<Appconf.filler.maxnum>50</Appconf.filler.maxnum>
    	</configuration>
		</leaf>
		
		<!--浅度补白-->
		<leaf name="guessyoulike-NewComputeFillerDriver">
    	<class>com.eb.bi.rs.mras.bookrec.guessyoulike.filler.NewComputeFillerDriver</class>
   		<configuration>
        	<!--用户推荐结果-->
        	<hdfs.input.path.result>/user/recsys/bookrec/geussyoulike/output_lowuser</hdfs.input.path.result>
