<leaf name="prefmatch">
    <class>com.eb.bi.rs.mras.bookrec.guessyoulike.PreferenceMatchDriver</class>
    <configuration>
    	<!--打分数据输入路径-->
        <conf.pref.path>guessyoulike/pref</conf.pref.path>
        <!--来源连接后的数据输入路径-->
        <conf.src.path>guessyoulike/srcconcat</conf.src.path>
        <!--输出路径，结果为sequencefile，key为user|book，value为pref|src1,src2...-->
        <conf.output.path>guessyoulike/prefmatch</conf.output.path>
        <!--reducer个数，默认为1-->
        <conf.num.reduce.tasks>1</conf.num.reduce.tasks>
    </configuration>
</leaf>