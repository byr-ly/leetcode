package com.eb.bi.rs.opusstorm.entity;

/**
 * 动漫基本信息
 */
public class OpusInfo {

	private String opusId;
	private String opusType;
	private String subjectId;
	private String author;
	private String contentSour; // （普通0、原创1）
	private String rank;

	public OpusInfo() {

	}
	
	public OpusInfo(String opusId, String opusType, String rank) {
		this.opusId = opusId;
		this.opusType = opusType;
		this.rank = rank;
	}

	public OpusInfo(String opusId, String opusType, String subjectId,
			String author, String contentSour, String rank) {
		this.opusId = opusId;
		this.opusType = opusType;
		this.subjectId = subjectId;
		this.author = author;
		this.contentSour = contentSour;
		this.rank = rank;
	}

	public String getOpusId() {
		return opusId;
	}

	public void setOpusId(String opusId) {
		this.opusId = opusId;
	}

	public String getOpusType() {
		return opusType;
	}

	public void setOpusType(String opusType) {
		this.opusType = opusType;
	}

	public String getSubjectId() {
		return subjectId;
	}

	public void setSubjectId(String subjectId) {
		this.subjectId = subjectId;
	}

	public String getAuthor() {
		return author;
	}

	public void setAuthor(String author) {
		this.author = author;
	}

	public String getContentSour() {
		return contentSour;
	}

	public void setContentSour(String contentSour) {
		this.contentSour = contentSour;
	}

	public String getRank() {
		return rank;
	}

	public void setRank(String rank) {
		this.rank = rank;
	}

}
