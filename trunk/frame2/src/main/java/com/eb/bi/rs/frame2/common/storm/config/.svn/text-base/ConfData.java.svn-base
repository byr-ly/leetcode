package com.eb.bi.rs.frame2.common.storm.config;

public class ConfData {
    private String type;
    private String link;
    private String command;
    private String meta;

    public ConfData(String type, String link, String command, String meta) {
        this.type = type;
        this.link = link;
        this.command = command;
        this.meta = meta;
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public String getLink() {
        return link;
    }

    public void setLink(String link) {
        this.link = link;
    }

    public String getLoadCommand() {
        return command;
    }

    public void setLoadCommand(String command) {
        this.command = command;
    }

    public String getMeta() {
        return meta;
    }

    public void setMeta(String link) {
        this.link = meta;
    }

    @Override
    public String toString() {
        return "DataMeta={[type=" + type + "] [link=" + link
                + "] [loadCommand=" + command + "] [meta=" + meta + "]}";
    }

}
