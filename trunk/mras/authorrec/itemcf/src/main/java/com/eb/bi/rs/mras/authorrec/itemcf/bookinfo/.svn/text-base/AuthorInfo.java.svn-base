package com.eb.bi.rs.mras.authorrec.itemcf.bookinfo;

public class AuthorInfo {

    private String authorName = null;
    private String penName = null;
    private String famous;
    private String authorid = null;

    public AuthorInfo(String id, String name, String pen, String f) {
        this.authorid = id;
        this.authorName = name;
        this.penName = pen;
        this.famous = f;
    }

    public String toString() {
        String str = String.format("%s|%s|%s|%s", authorid, authorName, penName, famous);
        return str;
    }
}
